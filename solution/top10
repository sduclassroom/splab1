#! /bin/bash

# run this script as "top10 log.txt"

# here comes your solution
#! /bin/bash 
lines=$(cut -f11 -d" " log.txt | grep -v '"-"' | wc -l);
echo $lines;
for i in `cut -f11 -d" " $1| grep -v '"-"' | sort -t" " -k1 | uniq -c | sort --reverse -k1 -n | head`;
do if [[ $i =~ ^[0-9]+$ ]]; then
	 printf "$i -  0$(echo "scale=4; $i / $lines" | bc)%%" ;
   else printf " - $i \n"; fi  
done 
# e.g.
grep world $1
